{
    "name": "common-coding-tools",
    "version": "2.0.5",
    "description": "A common code module for use in multiple npm packages",
    "repository": "PaneruVipin/common-coding-tools",
    "main": "src/index.js",
    "typings": "src/index.d.ts",
    "files": [
        "src"
    ],
    "scripts": {
        "test": "jest"
    },
    "keywords": [
        "common",
        "coding",
        "tools",
        "common hooks",
        "javascript",
        "validation",
        "data fitlerization",
        "types",
        "string metods",
        "backend validation",
        "hashing",
        "converter fn",
        "array methodes"
    ],
    "author": "PaneruVipin",
    "funding": [
        {
            "type": "individual",
            "url": "https://opencollective.com/common-coding-tools"
        }
    ],
    "license": "ISC",
    "exports": {
        ".": {
            "types": "./src/index.d.ts",
            "import": "./src/index.js",
            "require": "./src/index.js"
          },
        "./array": {
            "import":"./src/_array.js",
            "require":"./src/_array.js",
            "types":"./src/_array.d.ts"
        },
        "./string":{
            "import": "./src/string.js",
            "require": "./src/string.js",
            "types": "./src/string.d.ts"
        },
        "./validation":{
            "import": "./src/validation.js",
            "require": "./src/validation.js",
            "types": "./src/validation.d.ts"
        },
        "./types":{
            "import": "./src/types.js",
            "require": "./src/types.js",
            "types": "./src/types.d.ts"
        },
        "./hashing":{
            "import": "./src/hash.js",
            "require": "./src/hash.js",
            "types": "./src/hash.d.ts"
        },
        "./random":{
            "import": "./src/random.js",
            "require": "./src/random.js",
            "types": "./src/random.d.ts"
        }
    },
    "devDependencies": {
        "typescript": "^4.9.4"
    }
}
