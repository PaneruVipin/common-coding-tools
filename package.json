        },
        "./array": {
            "types": "./array/index.d.ts",
            "import": "./array/index.js",
            "require": "./array/index.js"
        },
        "./string": {
            "types": "./string/index.d.ts",
            "import": "./string/index.js",
            "require": "./string/index.js"
