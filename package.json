        "./validation": {
            "types": "./validation/index.d.ts",
            "import": "./validation/index.js",
            "require": "./validation/index.js"
        },
        "./types": {
            "types": "./types/index.d.ts",
            "import": "./types/index.js",
            "require": "./types/index.js"
        },
        "./hashing": {
            "types": "./hashing/index.d.ts",
            "import": "./hashing/index.js",
            "require": "./hashing/index.js"
        },
        "./random": {
            "types": "./random/index.d.ts",
            "import": "./random/index.js",
            "require": "./random/index.js"
        }
    },
    "devDependencies": {
        "typescript": "^4.9.4"
    }
