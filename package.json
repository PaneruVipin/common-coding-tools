    },
    "keywords": [
        "common",
        "coding",
        "tools",
        "common hooks",
        "javascript",
        "validation",
        "data fitlerization",
        "types",
        "string metods",
        "backend validation",
        "hashing",
        "converter fn",
        "array methodes"
    ],
    "author": "PaneruVipin",
    "funding": [
        {
            "type": "individual",
            "url": "https://opencollective.com/common-coding-tools"
        }
    ],
    "license": "ISC",
    "exports": {
        ".": {
            "types": "./src/index.d.ts",
            "import": "./src/index.js",
